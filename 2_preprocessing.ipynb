    "# visualization\n",
    "if correlate:\n",
    "    # Compute correlation matrix\n",
    "    feat_df = merged_df.drop(columns=['HADM_ID', 'readmission_30'])\n",
    "    corr = feat_df.corr()\n",
    "    fig, ax = plt.subplots(figsize=(12,10))\n",
    "    cax = ax.matshow(corr, vmin=-1, vmax=1)\n",
    "    fig.colorbar(cax)\n",
    "\n",
    "    ticks = np.arange(len(feat_df.columns))\n",
    "    ax.set_xticks(ticks)\n",
    "    ax.set_yticks(ticks)\n",
    "    ax.set_xticklabels(feat_df.columns, rotation=90, fontsize=5)\n",
    "    ax.set_yticklabels(feat_df.columns, fontsize=5)\n",
    "\n",
    "    plt.title(\"Correlation Heatmap (All Features)\", pad=20)\n",
    "    plt.savefig(\"../figures/correlation_heatmap.png\")\n",
    "\n",
    "if boxplot:\n",
    "    # select k=4 best\n",
    "    k =4\n",
    "    X_sel = merged_df.drop(columns=['HADM_ID', 'readmission_30'])\n",
    "    y_sel = merged_df['readmission_30']\n",
    "    # impute missing values\n",
    "    imputer = SimpleImputer(strategy='median')\n",
    "    X_sel_imputed = pd.DataFrame(imputer.fit_transform(X_sel), columns=X_sel.columns, index=X_sel.index)\n",
    "    selector = SelectKBest(f_classif, k=min(k, X_sel_imputed.shape[1]))\n",
    "    selector.fit(X_sel_imputed, y_sel)\n",
    "    key_feats = X_sel_imputed.columns[selector.get_support()]\n",
    "    print(f\"Top {len(key_feats)} features selected by SelectKBest:\")\n",
    "    print(\", \".join(key_feats))\n",
    "\n",
    "    ncols = 2\n",
    "    nrows = math.ceil(len(key_feats) / ncols)\n",
    "    fig, axes = plt.subplots(nrows, ncols, figsize=(12, 5 * nrows), sharey=False)\n",
    "    # make sure axes is a flat 1‑D array for easy iteration\n",
    "    if isinstance(axes, np.ndarray):\n",
    "        axes = axes.flatten()\n",
    "    else:                       # when only one subplot\n",
    "        axes = np.array([axes])\n",
    "    for ax, feat in zip(axes, key_feats):\n",
    "        no = merged_df.loc[merged_df['readmission_30'] == 0, feat]\n",
    "        yes = merged_df.loc[merged_df['readmission_30'] == 1, feat]\n",
    "        ax.boxplot([no.dropna(), yes.dropna()], tick_labels=['No', 'Yes'])\n",
    "        # Show feature name under the boxes for clarity\n",
    "        ax.set_title(\"\")  # no title at top\n",
    "        ax.text(0.5, -0.25, feat, transform=ax.transAxes,\n",
    "                ha='center', va='top', fontsize=9)\n",
    "\n",
    "    # Remove unused subplots if any\n",
    "    for ax in axes[len(key_feats):]:\n",
    "        fig.delaxes(ax)\n",
    "    axes[0].set_ylabel(\"Value\")\n",
    "    plt.suptitle(f\"Top {len(key_feats)} Features by ANOVA F‑score\", y=0.92)\n",
    "    plt.tight_layout(rect=[0, 0, 1, 0.95])\n",
    "    plt.savefig(\"../figures/boxplot_4.png\")\n",
    "    plt.show()\n",
    "\n",
    "if visualize:\n",
    "    # ------------------------------------------------------------------\n",
    "    # 2.  Helper formatters\n",
    "    # ------------------------------------------------------------------\n",
    "    def fmt_mean_std(series: pd.Series) -> str:\n",
    "        \"\"\"Return “μ ± σ” with two-decimal precision (NaNs ignored).\"\"\"\n",
    "        return f\"{series.mean(skipna=True):.2f} ± {series.std(skipna=True):.2f}\"\n",
    "\n",
    "\n",
    "    def fmt_count_pct(count: int, total: int) -> str:\n",
    "        \"\"\"Return “n (pct%)” for counts such as Gender rows.\"\"\"\n",
    "        return f\"{count} ({count / total * 100:.2f}%)\"\n",
    "\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    # 3.  Column (sub-population) definitions\n",
    "    # ------------------------------------------------------------------\n",
    "    groups = {\n",
    "        \"Readmission Within 30 Days\": merged_df[merged_df[\"readmission_30\"] == 1],\n",
    "        \"No Readmission Within 30 Days\": merged_df[merged_df[\"readmission_30\"] == 0],\n",
    "        \"Total\": merged_df\n",
    "    }\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    # 4.  Row definitions\n",
    "    # ------------------------------------------------------------------\n",
    "    # 4-a Demographics\n",
    "    rows = {}\n",
    "\n",
    "    for col_name, code in [(\"Gender (Male)\", 1), (\"Gender (Female)\", 0)]:\n",
    "        for label, g in groups.items():\n",
    "            rows.setdefault(col_name, {})[label] = fmt_count_pct(\n",
    "                (g[\"GENDER_code\"] == code).sum(), len(g)\n",
    "            )\n",
    "\n",
    "    for label, g in groups.items():\n",
    "        rows.setdefault(\"Age\", {})[label] = fmt_mean_std(g[\"AGE\"])\n",
    "\n",
    "    # 4-b “stay”-level chart-value means   --------------------------------\n",
    "    chart_feats = [\n",
    "        \"HeartRate_mean_stay\", \"RespRate_mean_stay\", \"SBP_mean_stay\",\n",
    "        \"DBP_mean_stay\", \"Temp_mean_stay\", \"SpO2_mean_stay\",\n",
    "        \"BUN_mean_stay\", \"Creatinine_mean_stay\", \"Glucose_mean_stay\",\n",
    "        \"WBC_mean_stay\", \"GCS_Eye_mean_stay\", \"GCS_Verbal_mean_stay\",\n",
    "        \"GCS_Motor_mean_stay\"\n",
    "    ]\n",
    "\n",
    "    for feat in chart_feats:\n",
    "        nice_name = feat.replace(\"_mean_stay\", \"\").replace(\"_\", \" \")\n",
    "        for label, g in groups.items():\n",
    "            rows.setdefault(nice_name, {})[label] = fmt_mean_std(g[feat])\n",
    "\n",
    "    # 4-c  Other numeric variables  --------------------------------------\n",
    "    extra_map = {\n",
    "        \"total_cpt_count\": \"CPT Count\",\n",
    "        \"unique_diag_count\": \"Diagnosis Count\",\n",
    "        \"mean_icu_los_hours\": \"ICU Length of Stay (h)\",\n",
    "        \"SOFA\": \"SOFA\",\n",
    "        \"SAPSII\": \"SAPS II\"\n",
    "    }\n",
    "\n",
    "    for raw_col, nice_name in extra_map.items():\n",
    "        for label, g in groups.items():\n",
    "            rows.setdefault(nice_name, {})[label] = fmt_mean_std(g[raw_col])\n",
    "\n",
    "    # ------------------------------------------------------------------\n",
    "    # 5.  Assemble final table\n",
    "    # ------------------------------------------------------------------\n",
    "    table1 = (\n",
    "        pd.DataFrame(rows)\n",
    "        .T  # variables as rows\n",
    "        .reindex([\n",
    "            # --- Demographics\n",
    "            \"Age\", \"Gender (Male)\", \"Gender (Female)\",\n",
    "            # --- Chart values\n",
    "            \"HeartRate\", \"RespRate\", \"SBP\", \"DBP\", \"Temp\", \"SpO2\",\n",
    "            \"BUN\", \"Creatinine\", \"Glucose\", \"WBC\",\n",
    "            \"GCS Eye\", \"GCS Verbal\", \"GCS Motor\",\n",
    "            # --- Utilisation & severity\n",
    "            \"CPT Count\", \"Diagnosis Count\", \"ICU Length of Stay (h)\",\n",
    "            \"SOFA\", \"SAPS II\"\n",
    "        ])\n",
    "        .loc[:, list(groups.keys())]  # keep column order\n",
    "    )\n",
    "\n",
    "    # 6.  Save / display\n",
    "    table1.to_csv(\"../figures/table1_summary.csv\")\n",
    "    print(table1)\n",
    "\n",
    "    # ------------------------------------------------------------\n",
    "    # Build hierarchical display (first‑tier headers + indented rows)\n",
    "    # ------------------------------------------------------------\n",
    "    row_order = [\n",
    "        \"Demographics\",\n",
    "        \"  Age\",\n",
    "        \"  Gender (Male)\",\n",
    "        \"  Gender (Female)\",\n",
    "        \"Chart Values\",\n",
    "        \"  HeartRate\", \"  RespRate\", \"  SBP\", \"  DBP\", \"  Temp\", \"  SpO2\",\n",
    "        \"  BUN\", \"  Creatinine\", \"  Glucose\", \"  WBC\",\n",
    "        \"  GCS Eye\", \"  GCS Verbal\", \"  GCS Motor\",\n",
    "        \"CPT Count\",\n",
    "        \"Diagnosis Count\",\n",
    "        \"ICU Length of Stay (h)\",\n",
    "        \"Severity Marker\",\n",
    "        \"  SOFA\",\n",
    "        \"  SAPS II\",\n",
    "    ]\n",
    "\n",
    "    # Look up rows without leading spaces, then re‑assign the indented index\n",
    "    display_df = table1.loc[[r.strip() for r in row_order]].copy()\n",
    "    display_df.index = row_order\n",
    "\n",
    "    fig_height = len(display_df) * 0.35 + 1  # auto-size rows\n",
    "\n",
    "    fig, ax = plt.subplots(figsize=(10, fig_height))\n",
    "    ax.axis(\"off\")\n",
    "\n",
    "    tbl = ax.table(cellText=display_df.values,\n",
    "                   colLabels=display_df.columns,\n",
    "                   rowLabels=display_df.index,\n",
    "                   loc=\"center\")\n",
    "    # Highlight first‑tier rows (those without leading spaces)\n",
    "    ncols = len(display_df.columns)\n",
    "    for i, label in enumerate(display_df.index, start=0):\n",
    "        if not label.startswith(\"  \"):              # first‑tier header\n",
    "            for j in range(ncols + 1):              # +1 for row‑label column\n",
    "                cell = tbl[i + 1, j]                # +1 because header row is row‑0 in mpl‑table\n",
    "                cell.set_text_props(weight=\"bold\")\n",
    "                cell.set_facecolor(\"#ececec\")\n",
    "\n",
    "\n",
    "    # styling tweaks\n",
    "    tbl.auto_set_font_size(False)\n",
    "    tbl.set_fontsize(8)\n",
    "    tbl.scale(1, 1.15)  # row height\n",
    "\n",
    "    plt.tight_layout()\n",
    "\n",
    "    # ────────────────────────────────────────────────────────────────\n",
    "    # 3)  Save & show\n",
    "    # ────────────────────────────────────────────────────────────────\n",
    "    out_png = \"../figures/table1_summary_plot.png\"\n",
    "    plt.savefig(out_png, dpi=300, bbox_inches=\"tight\")\n",
    "    plt.show()\n",
    "\n",
    "\n"
   ],
